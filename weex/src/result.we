dth:40px;        height: 40px;        margin-top: 14px;   }   .cancel{        align-items: center;        font-size: 30px;        color: #ffffff;   }   .query-top{     margin-top: 8px;     font-size: 28px;     justify-content:space-between;     flex-direction: row;     margin-left:55px;   }   .query-top-left{       flex-direction: row;   }   .q-active{      color: #c8050c;   }   .active{       color: #c8050c;       border-bottom-width:2px;       border-bottom-color:#c8050c;   }   .query-top-right{       flex-direction: row;       padding-right: 20px;   }   .qr-top{       padding-top: 20px;       font-size: 28px;   }   .arrow{       width: 40px;       height: 40px;       margin-top: 15px;   }   .tl-text{      width: 82px;      height: 70px;      padding-top: 20px;      font-size: 28px;      text-align: center;      border-bottom-width:0px;      color: #000000;   }   .query-condition{      flex-direction: row;      height: 80px;   }   .query-area{     visibility: hidden;     height: 0;     margin-top: 27px;   }   .qy-title{     font-size: 24px;     width: 120px;     lines:1   }   .qy-all{        font-size: 24px;        lines:1;        width:80px;   }   .qy-text{     font-size: 24px;     lines:1;     width:140px;   }   .hot-search-list{      margin-top: 27px;   }   .hot-search-list-one{     flex-direction: row;     height: 80px;     padding-left: 44px;     align-items: center;   }    .first{        background-color: #ff3c3c;    }    .second{      background-color: #ff873c;    }    .third{       background-color: #ffc83c;    }   .sort{      width: 32px;      text-align: center;      color: #fff;      font-size: 26px;      height:32px;      background-color: #d9d9d9;   }   .hot-search-text{        font-size: 26px;        flex: 1;        lines:1;        padding-left: 10px;   }   .qy-rt-img{      width: 194px;      height: 258px;      left: 76px;      top: 22px;  