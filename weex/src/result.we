._ids.qArea.el.setClassStyle({visibility:"visible",height:250,marginTop:27,marginLeft:68});                 this.resultHeight=screenHeight-427;            }else{                 this._ids.qArea.el.setClassStyle({visibility:"hidden",height:0,marginTop:27});                 this.resultHeight=screenHeight-170;            }            this.rotate(this._ids.arrow.el.ref);         },         aboutClick:function(e) {                var vid = e.target.attr.vid;                this.updateStatus(vid,this.condition.about);         },         updateStatus:function(id,obj) {            obj.status=id;         },         rotate: function(ref, callback) {             var self = this;             if(!self.current_rotate) {                 self.current_rotate = 0;             }             self.current_rotate = self.current_rotate + 180;             self.anim(ref, {                 transform: 'rotate(' + self.current_rotate + 'deg)'             }, 'linear', 100, callback);         },         anim: function(ref, styles, timingFunction, duration, callback) {             animation.transition(ref, {               styles: styles,               timingFunction: timingFunction,               duration: duration             }, callback || function(){});         }       }  }</script>